(function($) { 'use strict';

  $('.stat__number').each(function(i) {
      var target = this;
      var siblings = $(this).siblings('.has__plus');
      var stat = $(this).text().replace(/,/g, '');
      var statEnd = +stat;
      var statStart = stat / 1.08;
      var addPlus = function() {
        $(target).addClass('add__plus');
      };

      var counter1 = new CountUp(target, statStart, statEnd, 0, 1.2+i, {ââ
        useEasing: true,
        useGrouping: true,
        separator: ',',
        decimal: '.'
      });

      var waypoint1 = new Waypoint({
        element: this,
        handler: function(direction) {
          if (direction == "up") {
            counter1.reset();
          } else {

            if(siblings.length > 0) { 
              counter1.start(addPlus);
            } else {
              counter1.start();
            }
            
          }
        },
        offset: '110%'
      });

  });

})(jQuery);;
